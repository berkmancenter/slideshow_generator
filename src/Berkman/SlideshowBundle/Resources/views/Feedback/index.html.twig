{% extends 'BerkmanSlideshowBundle::layout.html.twig' %}

{% block body %}
<div class="page">
    <div class="lightWrap">
        <form action="{{ path('BerkmanSlideshowBundle_feedback') }}" method="post" {{ form_enctype(form) }}>
        <fieldset>
        <legend>Feedback</legend>
        {{ form_widget(form) }}
        </fieldset>
        <div><button type="submit">Send</button></div>
    </div>
</div>
{% endblock %}
